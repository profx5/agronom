[flake8]
max-line-length = 120
max-complexity = 10
# ignores for django settings
ignore =
    # F401 '.security.*' imported but unused
    F401,
    # F403 unable to detect undefined names
    F403,
    # F405 may be undefined, or defined from star imports
    F405,
    # F821 undefined name
    F821,
    # E402 module level import not at top of file
    E402
